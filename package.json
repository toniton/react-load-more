{
  "name": "react-load-more-hook",
  "version": "1.0.5",
  "description": "This is a react hook to help you implement infinite scroll or load more feature on scroll to bottom of a page or element.",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "build": "tsc && grunt",
    "build:watch": "tsc -w && grunt watch",
    "test": "npm run test",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground:ww": "npm run build && concurrently \"npm run playground:build\" \"npm run lite\"",
    "playground": "tsc && concurrently \"npm run playground:build\" \"npm run lite\""
  },
  "keywords": [
    "Infinite",
    "Scroll",
    "Load",
    "Loading",
    "More"
  ],
  "author": "Toniton",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-systemjs": "^7.4.4",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.8.17",
    "@types/react-dom": "^16.8.4",
    "concurrently": "^4.1.0",
    "core-js-bundle": "^3.0.1",
    "eslint": "^5.16.0",
    "grunt": "^1.0.4",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-uglify": "^4.0.1",
    "grunt-contrib-watch": "^1.1.0",
    "lite-server": "^2.4.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "systemjs": "^3.1.6",
    "tslint": "^5.16.0",
    "typescript": "^3.4.5",
    "zone.js": "^0.9.1"
  },
  "dependencies": {}
}
